{
  "name": "copilot",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "activationEvents": [],
  "main": "./dist/extension.cjs",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codewithcheese-sidebar",
          "title": "Codewithcheese",
          "icon": "cheese.svg"
        }
      ]
    },
    "views": {
      "codewithcheese-sidebar": [
        {
          "type": "webview",
          "id": "codewithcheese.sidebar",
          "name": "Codewithcheese"
        }
      ]
