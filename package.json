      },
      {
        "command": "codewithcheese.openChatPanel",
        "title": "Codewithcheese: New Chat"
      }
    ]
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "scripts": {
    "test:unit": "vitest --testTimeout=1000",
    "build": "node esbuild.mjs",
    "watch:esbuild": "node esbuild.mjs --watch"
